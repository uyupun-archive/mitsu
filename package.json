{
  "name": "mitsu",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "prod:up": "NODE_ENV=production forever start ./bin/www",
    "prod:ls": "forever list",
    "prod:down": "forever stop ./bin/www",
    "make:model": "sequelize model:create --underscored",
    "migrate": "sequelize db:migrate --env development --config ./configs/database.json",
    "migrate:fresh": "echo '' > stores/word2vec.sqlite3 & yarn migrate",
    "make:seeder": "sequelize seed:generate",
    "seed": "sequelize db:seed:all --config ./configs/database.json"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.6.1",
    "forever": "^3.0.2",
    "http-errors": "~1.6.3",
    "ioredis": "^4.17.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.12",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.3.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
